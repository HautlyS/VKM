{
  "version": "2.0.0",
  "templates": {
    "all-glm5": {
      "name": "ALL GLM-5 MODAL",
      "description": "Full GLM-5 multimodal setup across all providers with intelligent fallback",
      "icon": "üß†",
      "category": "multimodal",
      "author": "VKM Team",
      "tags": ["glm", "multimodal", "fallback", "zhipu", "openrouter"],
      "nodes": [
        {
          "id": "glm5-primary",
          "type": "service",
          "service": "zhipu",
          "model": "glm-5",
          "x": 10,
          "y": 10,
          "label": "GLM-5 Primary",
          "description": "Zhipu GLM-5 main endpoint"
        },
        {
          "id": "glm5-openrouter",
          "type": "service",
          "service": "openrouter",
          "model": "zhipu/glm-5",
          "x": 10,
          "y": 25,
          "label": "GLM-5 OpenRouter",
          "description": "OpenRouter GLM-5 mirror"
        },
        {
          "id": "key-zhipu",
          "type": "key",
          "service": "zhipu",
          "x": 40,
          "y": 10,
          "label": "Zhipu Key"
        },
        {
          "id": "key-openrouter",
          "type": "key",
          "service": "openrouter",
          "x": 40,
          "y": 25,
          "label": "OpenRouter Key"
        },
        {
          "id": "multimodal-router",
          "type": "router",
          "mode": "priority-failover",
          "x": 70,
          "y": 17,
          "label": "Multimodal Router",
          "description": "Routes to best available GLM-5 endpoint"
        },
        {
          "id": "openclaw-integration",
          "type": "integration",
          "integration": "openclaw",
          "x": 100,
          "y": 10,
          "label": "OpenClaw"
        },
        {
          "id": "claude-code-integration",
          "type": "integration",
          "integration": "claude-code",
          "x": 100,
          "y": 25,
          "label": "Claude Code"
        },
        {
          "id": "glm5-session",
          "type": "session",
          "x": 130,
          "y": 17,
          "label": "GLM-5 Session",
          "persistent": true
        }
      ],
      "connections": [
        { "from": "glm5-primary", "to": "key-zhipu", "label": "auth", "priority": 1 },
        { "from": "glm5-openrouter", "to": "key-openrouter", "label": "auth", "priority": 2 },
        { "from": "key-zhipu", "to": "multimodal-router", "label": "primary", "priority": 1 },
        { "from": "key-openrouter", "to": "multimodal-router", "label": "fallback", "priority": 2 },
        { "from": "multimodal-router", "to": "openclaw-integration", "label": "route" },
        { "from": "multimodal-router", "to": "claude-code-integration", "label": "route" },
        { "from": "openclaw-integration", "to": "glm5-session", "label": "out" },
        { "from": "claude-code-integration", "to": "glm5-session", "label": "out" }
      ],
      "config": {
        "autoFailover": true,
        "healthCheckInterval": 30,
        "failoverThreshold": 2,
        "loadBalance": false,
        "timeout": 30000
      }
    },
