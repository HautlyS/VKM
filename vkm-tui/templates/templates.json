        { "from": "kiro-failover-router", "to": "proxy-cache", "label": "response" },
        { "from": "proxy-cache", "to": "openclaw-integration", "label": "cached" },
        { "from": "proxy-cache", "to": "claude-code-integration", "label": "cached" },
        { "from": "proxy-cache", "to": "aider-integration", "label": "cached" },
        { "from": "openclaw-integration", "to": "kiro-primary-session", "label": "out" },
        { "from": "claude-code-integration", "to": "kiro-fallback-session", "label": "out" },
        { "from": "aider-integration", "to": "kiro-fallback-session", "label": "out" }
      ],
      "config": {
        "kiroPort": 8787,
        "autoStartKiro": true,
        "failoverDelay": 5000,
        "healthCheckInterval": 60,
        "cacheEnabled": true,
        "cacheTTL": 3600,
        "retryAttempts": 3
      }
    },
